<UserControl x:Class="CFAInmuebles.WPF.FichaTipoIVA"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CFAInmuebles.WPF"
             mc:Ignorable="d" 
             Template="{StaticResource UserControlMantenimientoVM}"
             Style="{StaticResource CustomStyleUserControl}"
             d:DesignHeight="450" d:DesignWidth="800">
    <UserControl.Resources >
        <DataTemplate DataType="{x:Type local:AltaTipoIVAVM}" >
            <local:AltaTipoIVA />
        </DataTemplate>
    </UserControl.Resources >
    <DockPanel Margin="20">
        <ContentControl  Margin="5" Content="{Binding Path=CurrentPageViewModel}" />
    </DockPanel>
</UserControl>

